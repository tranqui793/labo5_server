{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": [
            120
        ],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "quotemark": [
            true,
            "double",
            "avoid-escape"
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ],
        "arrow-parens": false,
        "no-console": [
            "debug",
            "info",
            "log",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-var-requires": false,
        "no-trailing-whitespace": false,
        "interface-name": [false, "always-prefix"],
        "eofline": false,
        "ordered-imports": [
            false
        ]
    },
    "jsRules": {
        "max-line-length": [
            120
        ]
    }
}